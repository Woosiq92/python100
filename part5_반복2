
# 60. 사각형 별찍기 

for i in range(4): 
    for j in range(4): 
        print("*", end='')
    print()   

# 61 삼각형 1  

for i in range(5):
    for j in range(i+1):
        print('*', end='')
    print()

#61-2 반복문 하나만 사용 

for i in range(1, 6): 
    print('*' * i )


#62 대각선 

for i in range(5): 
    for j in range(5): 
        if i == j : 
            print("*", end='')
        else: 
            print(' ', end='')
    print()
